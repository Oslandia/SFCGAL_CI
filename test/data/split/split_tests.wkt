# Split test cases - format: geometry|linestring
# Basic split tests
POLYGON((0 0,10 0,10 6,0 6,0 0))|LINESTRING(5 -1,5 7)
POLYGON((0 0,10 0,10 6,0 6,0 0))|LINESTRING(-1 3,11 3)
POLYGON((0 0,10 0,10 6,0 6,0 0))|LINESTRING(0 0,10 6)
# No-split cases
POLYGON((0 0,10 0,10 6,0 6,0 0))|LINESTRING(20 0,20 10)
POLYGON((0 0,10 0,10 6,0 6,0 0))|LINESTRING(0 0,-5 -5)
POLYGON((0 0,10 0,10 6,0 6,0 0))|LINESTRING(0 0,10 0)
# Vertex intersection tests
POLYGON((0 0,10 0,10 6,0 6,0 0))|LINESTRING(0 0,10 3)
# Polygon with hole
POLYGON((0 0,10 0,10 10,0 10,0 0),(3 3,3 7,7 7,7 3,3 3))|LINESTRING(5 -1,5 11)
# Asymmetric split
POLYGON((0 0,100 0,100 10,0 10,0 0))|LINESTRING(5 -1,5 11)
# Complex shapes
POLYGON((0 0,6 0,6 3,3 3,3 6,0 6,0 0))|LINESTRING(4 -1,4 7)
# Z coordinate interpolation
POLYGON Z((0 0 0,10 0 5,10 6 10,0 6 5,0 0 0))|LINESTRING Z(5 -1 0,5 7 0)
# M coordinate interpolation
POLYGON ZM((0 0 0 0,10 0 0 100,10 6 0 200,0 6 0 50,0 0 0 0))|LINESTRING ZM(5 -1 0 50,5 7 0 150)
# XYZ polygon with XY line
POLYGON Z((0 0 10,10 0 20,10 6 30,0 6 15,0 0 10))|LINESTRING(5 -1,5 7)
# XY polygon with XYZ line
POLYGON((0 0,10 0,10 6,0 6,0 0))|LINESTRING Z(5 -1 100,5 7 200)
# XYZ polygon with XYZM line
POLYGON Z((0 0 10,10 0 20,10 6 30,0 6 15,0 0 10))|LINESTRING ZM(5 -1 0 50,5 7 0 150)
# XYZM polygon with XY line
POLYGON ZM((0 0 10 0,10 0 20 100,10 6 30 200,0 6 15 50,0 0 10 0))|LINESTRING(5 -1,5 7)
# Triangle tests
TRIANGLE((0 0,10 0,5 8,0 0))|LINESTRING(5 -1,5 10)
TRIANGLE Z((0 0 10,10 0 20,5 8 30,0 0 10))|LINESTRING(5 -1,5 10)
TRIANGLE((0 0,10 0,5 8,0 0))|LINESTRING(20 -1,20 10)
# MultiPolygon tests
MULTIPOLYGON(((0 0,10 0,10 6,0 6,0 0)),((15 0,25 0,25 6,15 6,15 0)))|LINESTRING(5 -1,5 7)
MULTIPOLYGON Z(((0 0 5,10 0 10,10 6 15,0 6 8,0 0 5)),((15 0 20,25 0 25,25 6 30,15 6 22,15 0 20)))|LINESTRING(5 -1,5 7)
# PolyhedralSurface tests
POLYHEDRALSURFACE(((0 0,10 0,10 6,0 6,0 0)),((10 0,20 0,20 6,10 6,10 0)))|LINESTRING(5 -1,5 7)
POLYHEDRALSURFACE Z(((0 0 5,10 0 10,10 6 15,0 6 8,0 0 5)),((10 0 10,20 0 15,20 6 20,10 6 15,10 0 10)))|LINESTRING(5 -1,5 7)
# TriangulatedSurface tests
TIN(((0 0,10 0,5 8,0 0)),((5 8,10 0,10 8,5 8)))|LINESTRING(5 -1,5 10)
TIN Z(((0 0 10,10 0 20,5 8 30,0 0 10)),((5 8 30,10 0 20,10 8 35,5 8 30)))|LINESTRING(5 -1,5 10)
# GeometryCollection tests
GEOMETRYCOLLECTION(POLYGON((0 0,10 0,10 6,0 6,0 0)),POLYGON((15 0,25 0,25 6,15 6,15 0)),TRIANGLE((30 0,40 0,35 8,30 0)))|LINESTRING(5 -1,5 10)
GEOMETRYCOLLECTION Z(POLYGON Z((0 0 5,10 0 10,10 6 15,0 6 8,0 0 5)),TRIANGLE Z((15 0 20,25 0 25,20 8 30,15 0 20)))|LINESTRING(5 -1,5 10)
# Empty geometry tests
GEOMETRYCOLLECTION EMPTY|LINESTRING(5 -1,5 7)
POLYGON EMPTY|LINESTRING(5 -1,5 7)
POLYGON((0 0,10 0,10 6,0 6,0 0))|LINESTRING EMPTY
MULTIPOLYGON EMPTY|LINESTRING(5 -1,5 7)
POLYHEDRALSURFACE EMPTY|LINESTRING(5 -1,5 7)
TIN EMPTY|LINESTRING(5 -1,5 7)
